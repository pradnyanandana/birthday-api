{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [],
    "settings": {},
    "rules": {
        /* Add more rules if needed : https://eslint.org/docs/rules/ */
        "indent": [
            "warn",
            4,
            { "SwitchCase": 1 }
        ],
        "no-case-declarations": 0,
        "no-console": "warn",
        "no-eval": "error",
        "no-trailing-spaces": "warn",
        "no-unused-vars": "warn",
        "semi": "warn"
    }
}